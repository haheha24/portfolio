{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "portfolio root folder",
  "main": "index.js",
  "scripts": {
    "watch:server": "cd server && npm run watch",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "start:server": "cd server && npm run start",
    "dev:client": "cd client && npm run start",
    "dev:server": "cd server && npm run dev",
    "production": "npm run build && npm run start:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haheha24/portfolio.git"
  },
  "author": "adrian cristallo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haheha24/portfolio/issues"
  },
  "homepage": "https://github.com/haheha24/portfolio#readme",
  "devDependencies": {
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "concurrently": "^6.5.1"
  }
}
